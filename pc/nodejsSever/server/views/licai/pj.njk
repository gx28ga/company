                            {{ box.list(item) }}
                        {% endif %}
                    {% endfor %}
                </div>
                {# 期限down #}
                <div class="option">
                    {% for item in termDown %}
                        {% if item.type == 2 or item.type == 3 %}
                            {% import "./pj/box.njk" as box %}
                            {{ box.list(item) }}
                        {% endif %}
                    {% endfor %}
                </div>
            </div>
            {# end 全部产品 #}
            <div class="clearfix"></div>
        </article>
        <article class="container">
            {% include "./dq/topsort.njk" %}

            {# 全部产品 nodejs 生成 #}
            <div class="effect allProduct">
                {# 默认 #}
                <div class="option cur" style="display:block;">
                    {% for item in listDatas %}
                        {% if item.type == 5 %}
                            {% import "./pj/box.njk" as box %}
                            {{ box.list(item) }}
                        {% endif %}
                    {% endfor %}
                </div>
                {# 利率up #}
                <div class="option">
                    {% for item in rateUp %}
                        {% if item.type == 5 %}
                            {% import "./pj/box.njk" as box %}
