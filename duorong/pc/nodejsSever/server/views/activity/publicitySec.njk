        <a class="logoimg" href="http://www.duorongcf.com" target="_blank" style=""></a>
    </div>
    <div class="nav-box">
        <div>
            <ul>
                <li><img src="/images/activity/publicitySec/topright.png" alt=""></li>
            </ul>
        </div>
    </div>
</div>
<div id="APP" class="publicitySec">
    <div class="banner">
        <div class="regbox">
            <div class="reg">
                <form v-model="login" @click="clearEvent">
                    <div class="input-box">
                        <input type="tel" class="text" placeholder="请输入手机号码" v-model="login.mobilePhone" maxlength="11" >
                        <p class="error phoneErr">${phoneErr}</p>
                    </div>
                    <div class="input-box">
                        <input type="text" class="text" placeholder="请输入验证码" v-model="login.imgCode" >
                        <img :src="imgCodeUrl" @click="getImgCodeUrl" class="imgcode" alt="">
                        <p class="error">${imgCodeErr}</p>
                    </div>
                    <div class="input-box">
                        <input type="text" class="text" placeholder="请输入短信验证码" v-model="login.msg" >
                        <div class="getMsgBtn" @click="getSortMsg($event)">${sortName}</div>
                        <p class="error">${msgErr}</p>
                    </div>
                    <div class="input-box">
                        <input type="password" class="text" placeholder="请设置密码" v-model="login.pwd">
                        <p class="error">${passErr}</p>
                    </div>
                    <div class="recommend">
                        <p class="recommend-title"><i class="d" :class="{up:showRecommend}" @click="recommend"></i>推荐人（选填）</p>
                        <div class="input-box">
                            <input type="text" v-show="showRecommend" class="text" placeholder="请输入推荐人手机号码/推荐码" v-model="login.recommPhone" maxlength="11">
                            <p class="error"></p>
                        </div>
                    </div>
                    <div class="input-box agree" @click="check">
                        <div class="checked" :class="{check:!login.checkbox}"></div>
                        我已阅读并同意
                        <a href="/other/protocol/zhuceProtocol.html" target="_blank">《用户协议》</a>
